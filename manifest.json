{
    "manifest_version": 2,
    "name": "Bring me there",
    "description": "Automation for chrome.",
    "version": "0.0.1",
    "permissions": [
        "tabs",
        "management",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "icons": {
        "16": "img/bmt-16.png",
        "48": "img/bmt-48.png",
        "128": "img/bmt-128.png"
    },
    "background": {
        "page": "bg.html"
    },
    "browser_action": {
        "default_icon": {
            "19": "img/bmt-19.png",
            "38": "img/bmt-38.png"
        },
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "content_scripts": [{
        "all_frames": false,
        "js": [
            "js/contentscript.js"
        ],
        "matches": ["<all_urls>"],
        "match_about_blank": false,
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [
        "img/*.png",
        "js/*"
    ]
}
